{
  "manifest_version": 3,
  "name": "Redirect Network Chrome Extension",
  "version": "0.001",
  "content_scripts": [{ "matches": ["<all_urls>"], "js": ["content.js"] }],
  "action": {
    "default_popup": "popup-extension/build/index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "host_permissions": ["*://*/*"],
  "permissions": [
    "declarativeNetRequest",
    "webNavigation",
    "browserAction",
    "activeTab",
    "contextMenus",
    "storage"
  ]
}
